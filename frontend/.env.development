# BoliBooks Frontend Development Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_BASE_URL=http://localhost:3000

# App Configuration
REACT_APP_NAME=BoliBooks
REACT_APP_VERSION=1.0.0
REACT_APP_TITLE=BoliBooks - Complete Business Management Solution

# Payment Configuration (Test Keys)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
REACT_APP_PAYPAL_CLIENT_ID=your_test_paypal_client_id_here
REACT_APP_PAYPAL_ENVIRONMENT=sandbox

# Feature Flags
REACT_APP_ENABLE_REGISTRATION=true
REACT_APP_ENABLE_MULTI_CURRENCY=true
REACT_APP_ENABLE_WHATSAPP_SHARING=false
REACT_APP_ENABLE_STRIPE=true
REACT_APP_ENABLE_PAYPAL=true
REACT_APP_ENABLE_POS=true
REACT_APP_ENABLE_INVOICING=true

# Default Settings
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_DEFAULT_TAX_RATE=10.0
